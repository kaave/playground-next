{
  "name": "playground-next",
  "version": "0.1.0",
  "private": true,
  "description": "Next.js playground",
  "scripts": {
    "analyze": "cross-env BUNDLE_ANALYZE=both next build",
    "build": "next build",
    "build-storybook": "build-storybook",
    "createRobotsTxt": "node ./tools/createRobotsTxt",
    "createSitemap": "node ./tools/createSitemap",
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "export": "next export",
    "postexport": "run-s createRobotsTxt createSitemap remapHtmls",
    "fix": "run-p fix:*",
    "fix:json": "prettier --write '**/*.json'",
    "fix:script": "eslint --fix '**/*.{ts,tsx}'",
    "fix:style": "stylelint --fix '**/*.scss'",
    "lint": "run-p lint:*",
    "lint-staged": "lint-staged",
    "lint:script": "eslint '**/*.{ts,tsx}'",
    "lint:style": "stylelint '**/*.scss'",
    "remapHtmls": "node ./tools/remapHtmls",
    "server": "http-server ./out -p 3000",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "test": "cross-env NODE_ENV=test jest",
    "typecheck": "tsc --noEmit -p tsconfig.production.json",
    "typo": "cspell './src/**/*.{ts,tsx,scss}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-p typecheck lint-staged"
    }
  },
  "dependencies": {
    "@kaave/result": "^0.2.1",
    "@welldone-software/why-did-you-render": "^6.0.5",
    "abortcontroller-polyfill": "^1.7.1",
    "clsx": "^1.1.1",
    "date-fns": "^2.16.1",
    "isomorphic-unfetch": "^3.1.0",
    "next": "^10.0.5",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-transition-group": "^4.4.1",
    "regenerator-runtime": "^0.13.7",
    "stop-runaway-react-effects": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-numeric-separator": "^7.12.7",
    "@babel/plugin-proposal-private-methods": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@storybook/addon-a11y": "^6.1.15",
    "@storybook/addon-actions": "^6.1.15",
    "@storybook/addon-backgrounds": "^6.1.15",
    "@storybook/addon-docs": "^6.1.15",
    "@storybook/addon-knobs": "^6.1.15",
    "@storybook/addon-links": "^6.1.15",
    "@storybook/addon-viewport": "^6.1.15",
    "@storybook/addons": "^6.1.15",
    "@storybook/react": "^6.1.15",
    "@storybook/source-loader": "^6.1.15",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/react-hooks": "^5.0.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-transition-group": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "autoprefixer": "^10.2.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "cspell": "^5.2.3",
    "css-loader": "^5.0.1",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^6.0.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-redux": "^3.3.2",
    "eslint-plugin-unicorn": "^27.0.0",
    "fibers": "^5.0.0",
    "fs-extra": "^9.1.0",
