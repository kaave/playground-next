    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "test": "cross-env NODE_ENV=test jest",
    "typecheck": "tsc --noEmit -p tsconfig.production.json",
    "typo": "cspell './src/**/*.{ts,tsx,scss}'"
